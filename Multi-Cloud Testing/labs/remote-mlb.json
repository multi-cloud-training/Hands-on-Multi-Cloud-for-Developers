  "cpus": 2,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": false,
      "image": "mesosphere/marathon-lb:v1.12.3",
      "parameters": [
        {
          "key": "label",
          "value": "created_by=marathon"
        },
        {
          "key": "label",
          "value": "dcos_pkg_name=marathon-lb"
        }
      ],
      "privileged": true
    },
    "volumes": []
