      "intervalSeconds": 5,
      "maxConsecutiveFailures": 2,
      "portIndex": 2,
      "timeoutSeconds": 2,
      "delaySeconds": 15,
      "protocol": "MESOS_HTTP",
      "path": "/_haproxy_health_check",
      "ipProtocol": "IPv4"
    }
  ],
